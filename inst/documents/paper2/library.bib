issn = {14710056},
journal = {Nature Reviews Genetics},
number = {6},
pages = {405--414},
pmid = {20479772},
publisher = {Nature Publishing Group},
title = {{The population genetics of antibiotic resistance: Integrating molecular mechanisms and treatment contexts}},
url = {http://dx.doi.org/10.1038/nrg2778},
volume = {11},
year = {2010}
}
