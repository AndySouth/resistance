issn = {20754450},
journal = {Insects},
keywords = {Aphid,Neonicotinoid,Organophosphate,Phenylpyrazol,Pyrethroid},
number = {4},
pages = {1--7},
title = {{The differential effect of low-dose mixtures of four pesticides on the pea aphid {\textless}i{\textgreater}Acyrthosiphon pisum{\textless}/i{\textgreater}}},
volume = {7},
year = {2016}
