number = {3},
pages = {187--196},
pmid = {26826784},
publisher = {Elsevier Ltd},
title = {{Insecticide Resistance in African Anopheles Mosquitoes: A Worsening Situation that Needs Urgent Action to Maintain Malaria Control}},
url = {http://dx.doi.org/10.1016/j.pt.2015.11.010},
volume = {32},
