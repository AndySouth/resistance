doi = {10.1371/journal.pntd.0005656},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/Vazquez2017{\_}deltamethrin-resistance-aedes-results-in-spray-failure-mexico.pdf:pdf},
