isbn = {1752-4571},
issn = {17524571},
journal = {Evolutionary Applications},
keywords = {Drugs,Evolution of resistance,Mathematical modelling,Pesticides,Resistance to xenobiotics},
number = {4},
pages = {375--390},
pmid = {25567932},
title = {{The skill and style to model the evolution of resistance to pesticides and drugs}},
volume = {3},
