journal = {Aspects of Applied Biology},
keywords = {Biotechnology (General) [WW000] Pathogen,Pa,Pest},
pages = {189--196},
title = {{Strategies to delay the evolution of resistance in pests: dose rotations and induced plant defenses}},
url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS{\&}CSC=Y{\&}NEWS=N{\&}PAGE=fulltext{\&}D=caba4{\&}AN=19991109224 http://library.wur.nl/sfx{\_}local?sid=OVID:cabadb{\&}id=pmid:{\&}id=doi:{\&}issn=0265-1491{\&}isbn={\&}volume={\&}issue=53{\&}spage=189{\&}pages=189-196{\&}date=1999{\&}title=Aspects+of+Applied+Bio},
