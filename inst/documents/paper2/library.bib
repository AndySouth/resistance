file = {:C$\backslash$:/Dropbox/ueaHelix2017/ecocrop/papers{\_}ecocrop/portmann2010{\_}MIRCA-global-crop-areas.pdf:pdf},
isbn = {0886-6236},
issn = {08866236},
journal = {Global Biogeochemical Cycles},
keywords = {http://dx.doi.org/10.1029/2008GB003435, doi:10.102},
number = {1},
pages = {n/a--n/a},
title = {{MIRCA2000-Global monthly irrigated and rainfed crop areas around the year 2000: A new high-resolution data set for agricultural and hydrological modeling}},
