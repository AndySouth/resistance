isbn = {0022-0493},
issn = {0022-0493},
journal = {Journal of Economic Entomology},
keywords = {and,chrysomelidae,coleoptera,diabrotica virgifera vir-,gifera leconte,in the united states,is a se-,refuge strategy,resistance management,rious pest of corn,the western corn rootworm,toxin pyramid,western corn rootworm,zea mays l},
number = {1998},
pages = {849--860},
pmid = {20568632},
title = {{Modeling evolution of Diabrotica virgifera virgifera (Coleoptera: Chrysomelidae) to transgenic corn with two insecticidal traits.}},
volume = {103},
year = {2010}
}
