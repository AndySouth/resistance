@article{Kleinschmidt2018,
abstract = {Background: Scale-up of insecticide-based interventions has averted more than 500 million malaria cases since 2000. Increasing insecticide resistance could herald a rebound in disease and mortality. We aimed to investigate whether insecticide resistance was associated with loss of effectiveness of long-lasting insecticidal nets and increased malaria disease burden. Methods: This WHO-coordinated, prospective, observational cohort study was done at 279 clusters (villages or groups of villages in which phenotypic resistance was measurable) in Benin, Cameroon, India, Kenya, and Sudan. Pyrethroid long-lasting insecticidal nets were the principal form of malaria vector control in all study areas; in Sudan this approach was supplemented by indoor residual spraying. Cohorts of children from randomly selected households in each cluster were recruited and followed up by community health workers to measure incidence of clinical malaria and prevalence of infection. Mosquitoes were assessed for susceptibility to pyrethroids using the standard WHO bioassay test. Country-specific results were combined using meta-analysis. Findings: Between June 2, 2012, and Nov 4, 2016, 40 000 children were enrolled and assessed for clinical incidence during 1{\textperiodcentered}4 million follow-up visits. 80 000 mosquitoes were assessed for insecticide resistance. Long-lasting insecticidal net users had lower infection prevalence (adjusted odds ratio [OR] 0{\textperiodcentered}63, 95{\%} CI 0{\textperiodcentered}51–0{\textperiodcentered}78) and disease incidence (adjusted rate ratio [RR] 0{\textperiodcentered}62, 0{\textperiodcentered}41–0{\textperiodcentered}94) than did non-users across a range of resistance levels. We found no evidence of an association between insecticide resistance and infection prevalence (adjusted OR 0{\textperiodcentered}86, 0{\textperiodcentered}70–1{\textperiodcentered}06) or incidence (adjusted RR 0{\textperiodcentered}89, 0{\textperiodcentered}72–1{\textperiodcentered}10). Users of nets, although significantly better protected than non-users, were nevertheless subject to high malaria infection risk (ranging from an average incidence in net users of 0{\textperiodcentered}023, [95{\%} CI 0{\textperiodcentered}016–0{\textperiodcentered}033] per person-year in India, to 0{\textperiodcentered}80 [0{\textperiodcentered}65–0{\textperiodcentered}97] per person year in Kenya; and an average infection prevalence in net users of 0{\textperiodcentered}8{\%} [0{\textperiodcentered}5–1{\textperiodcentered}3] in India to an average infection prevalence of 50{\textperiodcentered}8{\%} [43{\textperiodcentered}4–58{\textperiodcentered}2] in Benin). Interpretation: Irrespective of resistance, populations in malaria endemic areas should continue to use long-lasting insecticidal nets to reduce their risk of infection. As nets provide only partial protection, the development of additional vector control tools should be prioritised to reduce the unacceptably high malaria burden. Funding: Bill {\&} Melinda Gates Foundation, UK Medical Research Council, and UK Department for International Development.},
author = {Kleinschmidt, Immo and Bradley, John and Knox, Tessa Bellamy and Mnzava, Abraham Peter and Kafy, Hmooda Toto and Mbogo, Charles and Ismail, Bashir Adam and Bigoga, Jude D. and Adechoubou, Alioun and Raghavendra, Kamaraju and Cook, Jackie and Malik, Elfatih M. and Nkuni, Zinga Jos{\'{e}} and Macdonald, Michael and Bayoh, Nabie and Ochomo, Eric and Fondjo, Etienne and Awono-Ambene, Herman Parfait and Etang, Josiane and Akogbeto, Martin and Bhatt, Rajendra M. and Chourasia, Mehul Kumar and Swain, Dipak K. and Kinyari, Teresa and Subramaniam, Krishanthi and Massougbodji, Achille and Ok{\^{e}}-Sopoh, Mariam and Ogouyemi-Hounto, Aurore and Kouambeng, Celestin and Abdin, Mujahid Sheikhedin and West, Philippa and Elmardi, Khalid and Cornelie, Sylvie and Corbel, Vincent and Valecha, Neena and Mathenge, Evan and Kamau, Luna and Lines, Jonathan and Donnelly, Martin James},
doi = {10.1016/S1473-3099(18)30172-5},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/kleinschmidt2018{\_}implications-insecticide-resistance-for-malaria-vector-control-WHO-study.pdf:pdf},
issn = {14744457},
journal = {The Lancet Infectious Diseases},
number = {18},
pages = {1--10},
pmid = {29650424},
title = {{Implications of insecticide resistance for malaria vector control with long-lasting insecticidal nets: a WHO-coordinated, prospective, international, observational cohort study}},
volume = {3099},
year = {2018}
}
