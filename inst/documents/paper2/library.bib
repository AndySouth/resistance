isbn = {0269-283X (Print)$\backslash$r0269-283X (Linking)},
issn = {0269283X},
journal = {Medical and Veterinary Entomology},
keywords = {Anopheles gambiae,Bednet,Bioassay,Culex quinquefasciatus,C{\^{o}}te d'Ivoire,Insecticide resistance,Malaria vector,Mansonia,Olyset,Permethrin,Polyethylene net,Pyrethroid,Vector control,Verandah trap hut},
number = {1},
