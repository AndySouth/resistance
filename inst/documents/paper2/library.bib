keywords = {Africa,Africa: epidemiology,Animals,Antimalarials,Antimalarials: therapeutic use,Child,Child, Preschool,Databases, Factual,Drug Resistance,Endemic Diseases,Endemic Diseases: prevention {\&} control,Endemic Diseases: statistics {\&} numerical data,Humans,Incidence,Insecticide-Treated Bednets,Insecticide-Treated Bednets: utilization,Insecticides,Malaria, Falciparum,Malaria, Falciparum: drug therapy,Malaria, Falciparum: epidemiology,Malaria, Falciparum: parasitology,Malaria, Falciparum: prevention {\&} control,Plasmodium falciparum,Plasmodium falciparum: drug effects,Prevalence,Risk Assessment},
