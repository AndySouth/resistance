doi = {10.1186/s13071-014-0548-9},
file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/assogba2014{\_}phenotypic-effects-ace-kdr-insecticide-resistance.pdf:pdf},
