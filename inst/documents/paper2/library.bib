file = {:C$\backslash$:/Dropbox/resistance LSTM/resistance{\_}papers/MATOWO2015-selection-of-insecticide-resistance-an-gamb.pdf:pdf},
isbn = {0269-283x},
issn = {13652915},
journal = {Medical and Veterinary Entomology},
keywords = {Ace-1,Bendiocarb,Bioassays,Carbamate,Mutation,Organophosphate,Synergists,kdr},
number = {1},
pages = {51--59},
pmid = {25537754},
title = {{Trends in the selection of insecticide resistance in Anopheles gambiae s.l. mosquitoes in northwest Tanzania during a community randomized trial of longlasting insecticidal nets and indoor residual spraying}},
volume = {29},
year = {2015}
}
