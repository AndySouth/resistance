@article{Gardner1999,
abstract = {The evolution of pesticide and drug resistance plagues agriculture and medicine worldwide. Resistance may stem from a single major gene or from quantitative sources, and both have been observed in the same species. The genetic basis of resistance plays a key role in treatment, as low concentrations facilitate the evolution of quantitative resistance, while high concentrations select for the rapid appearance of major gene resistance. When crop rotation or pesticide rotation is unfeasible (e.g. orchard crops or cross resistance to multiple pesticides), the authors propose and model a strategy of rotating low and high concentrations in alternate treatments. This strategy should delay resistance and extend pest control longer than using a series of constant low or high concentrations. The model framework was modified to investigate whether inducible (compared with constitutive) defence may also slow the evolution of pest resistance to plant secondary compounds, with possible implications for genetic engineering of pest resistant crops. The model led to predictions that plants with inducible defenses supported pests that evolved resistance more slowly than plants with constitutive defenses, suggesting that natural selection has favoured a strategy similar to pesticide concentration rotation.},
author = {Gardner, Shea N and Agrawal, A A and Gressel, Jonathan and Mangel, Marc},
