issn = {1475-2875},
journal = {Malaria Journal},
keywords = {Insecticide resistance,Malaria vectors,GPIRM,Resou,gpirm,insecticide resistance,malaria vectors,resource deficiencies},
number = {1},
pages = {173},
pmid = {25899397},
