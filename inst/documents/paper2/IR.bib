keywords = {Animals,Biological,Drug Combinations,Insecticide Resistance,Models,Pest Control,Pest Control: methods,Pesticides},
number = {5},
pages = {1263--9},
pmid = {2689487},
title = {{Managing resistance with multiple pesticide tactics: theory, evidence, and recommendations.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/2689487},
